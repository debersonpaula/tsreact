{
  "name": "tsrx",
  "description": "Typescript + React",
  "version": "0.0.5",
  "main": "index.js",
  "scripts": {
    "build": "npm run clear && npm run build:source && npm run build:binary && npm run build:tools && node ./deploy/post-build.js",
    "build:source": "tsc -p deploy/tsconfig.source.json",
    "build:binary": "webpack -p --colors --progress --config ./deploy/webpack.config.deploy.js",
    "build:tools": "tsc -d -p deploy/tsconfig.tools.json",
    "clear": "rm -rf ./dist",
    "deploy": "npm run build && cd dist/bin && npm publish",
    "deploy:next": "npm run build && cd dist/bin && npm publish --tag next",
    "sb:start": "node dist/bin/tsrx start sandbox/config/react.config.js",
    "sb:build": "node dist/bin/tsrx build sandbox/config/react.config.js",
    "sb:test": "node dist/bin/tsrx test sandbox/config/react.config.js",
    "sb:lib": "node dist/bin/tsrx library sandbox/config/react.config.lib.js",
    "sb:static": "node dist/bin/tsrx build sandbox/config/react.config.static.js",
    "tsc": "tsc"
  },
  "bin": {
    "tsrx": "./index.js"
  },
  "author": "D.A.Paula <deberson.paula@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@hot-loader/react-dom": "16.13.0",
    "@types/enzyme": "3.10.5",
    "@types/jest": "25.2.1",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@types/react-router-dom": "5.1.4",
    "@types/webpack-dev-server": "3.10.1",
    "awesome-typescript-loader": "5.2.1",
    "babel-jest": "25.4.0",
    "babel-loader": "8.1.0",
    "babel-plugin-transform-typescript-metadata": "0.3.0",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.5.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.4",
