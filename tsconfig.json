{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "outDir": "dist/out",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "lib": ["es5", "es2015", "es6", "dom"],
    "module": "commonjs",
    "sourceMap": false,
    "target": "es5",
    "paths": {
      "tsrx/*": ["dist/bin/*"]
    }
  },
  "exclude": ["node_modules", "sandbox/**/*"]
}
